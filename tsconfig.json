{
  "compilerOptions": {
    "module": "es6",
    "moduleResolution": "node",
    "target": "esnext",
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "noUnusedLocals": false
  },
  "exclude": ["node_modules", "public", ".cache"],
  "include": ["./src", "custom.d.ts"]
}